{
  "tasks": {
    "dev": "deno run --allow-all --watch server/index.tsx",
    "serve": "deno run --allow-all server/index.tsx"
  },
  "imports": {
    "hono": "npm:hono@^4.0.0",
    "hono/cors": "npm:hono@^4.0.0/cors",
    "hono/logger": "npm:hono@^4.0.0/logger"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["deno.window"],
    "strict": true
  }
}
