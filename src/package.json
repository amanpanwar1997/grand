{
  "name": "inchtomilez-digital-marketing",
  "version": "7.0.0",
  "description": "Inchtomilez Digital Marketing And Advertising Agency - Deep Cleaned & Optimized",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "dev:backend": "cd supabase/functions && deno run --allow-all --watch server/index.tsx",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "node scripts/build-ssg.js",
    "build:vite": "vite build",
    "build:only": "vite build",
    "build:snap": "react-snap",
    "build:custom": "npm run build:only && node scripts/custom-snapshot.js",
    "build:analyze": "vite build --mode analyze",
    "postbuild": "echo Build complete!",
    "preview": "vite preview --host",
    "type-check": "tsc --noEmit",
    "routes": "node scripts/generate-all-routes.js",
    "verify": "node scripts/verify-ssg.js",
    "diagnose": "node scripts/diagnose-ssg.js",
    "serve": "npx serve dist -p 3000",
    "backend": "cd supabase/functions && deno run --allow-all --watch server/index.tsx",
    "backend:test": "curl http://localhost:8000/make-server-9c8e64e4/health",
    "supabase:deploy": "supabase functions deploy make-server-9c8e64e4",
    "test:supabase": "node scripts/test-supabase.js",
    "sitemap": "node scripts/generate-sitemap.js",
    "indexing:fix": "node scripts/fix-indexing.js",
    "indexing:submit": "node scripts/indexnow-submit.js",
    "indexing:full": "npm run sitemap && npm run indexing:fix && npm run indexing:submit"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.1",
    "react-helmet-async": "^2.0.5",
    "motion": "^11.11.17",
    "lucide-react": "^0.454.0",
    "lenis": "^1.1.17",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4",
    "class-variance-authority": "^0.7.1",
    "@supabase/supabase-js": "^2.47.10"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "vite": "^5.4.10",
    "typescript": "^5.6.3",
    "tailwindcss": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "react-snap": "^1.23.0",
    "puppeteer": "^23.11.1",
    "serve-handler": "^6.1.5",
    "concurrently": "^9.1.0"
  },
  "engines": {
    "node": ">=18.0.0 <=22.0.0"
  },
  "reactSnap": {
    "inlineCss": true,
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage",
      "--disable-accelerated-2d-canvas",
      "--disable-gpu"
    ],
    "skipThirdPartyRequests": true,
    "cacheAjaxRequests": false,
    "crawl": true,
    "waitFor": 2000,
    "concurrency": 4,
    "removeBlobs": true,
    "removeScriptTags": false,
    "removeStyleTags": false,
    "preloadResources": true,
    "asyncScriptTags": true,
    "preconnectThirdParty": false,
    "include": [
